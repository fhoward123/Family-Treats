<!DOCTYPE html>
<html>
<head>
<% include ../partials/head.ejs %>
</head>
<body>
    <div>
        <nav>
            <% include ../partials/navMin.ejs %>
        </nav>
    </div>
    <div class="bg">
        <div class="container">
            <h4>log in</h4>
            <div class="col">
                <form class="top-margin col s3" action="/sessions" method="POST">
                    <div class="input-field col s3">
                        <input placeholder="Placeholder" id="username" type="text" class="validate" name="username" required>
                        <label for="username">user name</label>
                    </div>
                    <div class="input-field col s3">
                        <input id="password" type="password" class="validate" name="password" required>
                        <label for="password">password</label>
                    </div>
                    <button class="btn waves-effect waves-light btn-font" type="submit" name="action">submit</button>
                </form>
            </div>
        </div>
    </div>
    <footer class="footer">
        <% include ../partials/footer.ejs %>
    </footer>
</body>
</html>
